+++ rasqal-0.9.30/CMakeLists.txt	2013-12-16 19:27:36.937019300 +0100
@@ -0,0 +1,227 @@
+project(rasqal)
+
+cmake_minimum_required(VERSION 2.8)
+
+set(RASQAL_VERSION_MAJOR 0)
+set(RASQAL_VERSION_MINOR 9)
+set(RASQAL_VERSION_RELEASE 30)
