+
+set(CMAKE_MODULE_PATH "${PROJECT_SOURCE_DIR}/cmake")
+
+find_package(PCRE REQUIRED)
+find_package(Raptor 2.0.4 REQUIRED)
+
+option(BUILD_EXAMPLES "build the examples" ON)
+option(BUILD_TOOLS "build commandline utilities" ON)
