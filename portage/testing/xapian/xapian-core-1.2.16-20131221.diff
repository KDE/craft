+    set(xapian_tcpsrv_SRCS xapian-tcpsrv.cc)
+    add_executable(xapian-tcpsrv ${xapian_tcpsrv_SRCS})
+    target_link_libraries(xapian-tcpsrv getopt xapian)
+    list(APPEND _targets xapian-tcpsrv)
